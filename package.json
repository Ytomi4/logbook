{
  "name": "logbook",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "wrangler pages dev --proxy 5173 --port 8788 --compatibility-date 2024-12-01",
    "build": "tsc -b && vite build",
    "preview": "wrangler pages dev ./dist --local --persist",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "vitest",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply logbook-db --local",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Reading log application with timeline view",
  "dependencies": {
    "better-auth": "^1.4.7",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.11.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.10.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.4.7",
    "@cloudflare/workers-types": "^4.20251216.0",
    "@eslint/js": "^9.39.2",
    "@storybook/react": "^10.1.10",
    "@storybook/react-vite": "^10.1.10",
    "@storybook/test": "^10.1.10",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/ui": "^3.2.4",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.25",
    "globals": "^16.5.0",
    "jsdom": "^27.0.1",
    "prettier": "^3.7.4",
    "storybook": "^10.1.10",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vite": "^7.3.0",
    "vitest": "^3.2.4",
    "wrangler": "^4.55.0"
  }
}
